{
  "name": "react-twgl-boilerplate",
  "version": "0.1.0",
  "description": "This is boilerplate includes React, TWGL.js & sandbox of WebGL",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "boilerplate",
    "twgl",
    "universal-router"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shuta13/react-twgl-boilerplate.git"
  },
  "author": "did0es",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "webpack serve --mode development --open",
    "build": "yarn clean && webpack",
    "watch": "yarn clean && webpack --watch",
    "start": "webpack serve --mode production --open",
    "clean": "rimraf dist/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "history": "^5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "universal-router": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "babel-loader": "^8.2.2",
    "html-webpack-plugin": "^5.3.1",
    "http-server": "^0.12.3",
    "react-refresh": "^0.10.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.3",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  }
}
